{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "Default": "Data Source=Data/MyCourse.db"
  },

  "Courses": {
    "PerPage": 10,
    "Order": {
      "By": "Rating",
      "Ascending": false,
      "Allow": ["Title", "Rating", "CurrentPrice"]
    }
  }
}
